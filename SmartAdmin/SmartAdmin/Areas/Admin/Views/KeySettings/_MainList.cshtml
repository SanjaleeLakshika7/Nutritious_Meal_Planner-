@model IEnumerable<APIKey>

@{
    Layout = null;
}

<div class="table-responsive">
    <table class="table table-centered table-nowrap table-striped">
        <thead>
            <tr>
                <th class="d-none">@Html.DisplayNameFor(model => model.First().KeyID)</th>
                <th>@Html.DisplayNameFor(model => model.First().KeyValue)</th>
                <th>@Html.DisplayNameFor(model => model.First().KeyDetails)</th>
                <th>@Html.DisplayNameFor(model => model.First().CreatedDate)</th>
                <th>@Html.DisplayNameFor(model => model.First().ActiveStatusText)</th>
                <th style="width:70px;">Action</th>
            </tr>
        </thead>
        <tbody id="tblBody">
            @foreach (var item in Model)
            {
            <tr id="row_@item.KeyID">
                <td class="d-none">@Html.DisplayFor(modelItem => item.KeyID)</td>
                <td>@Html.DisplayFor(modelItem => item.KeyValue)</td>
                <td title="@item.KeyDetails">@item.KeyDetails.ToShort(36)</td>
                <td>@Html.DisplayFor(modelItem => item.CreatedDate)</td>
                <td><span class="badge badge-@item.ActiveStatusClass">@item.ActiveStatusText</span></td>
                <td>
                    <a href="@Url.Action("Edit", new { id = item.KeyID })" class="table-icon text-primary"><i class="mdi mdi-square-edit-outline"></i></a>
                    <a href="javascript:void(0);" onclick="ConfirmDelete('@item.KeyID','@Url.Action("Delete")')" class="table-icon text-danger"> <i class="mdi mdi-delete"></i></a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>



