@{
    ViewData["Title"] = "Sub Categories";
    ViewData["TitleNavbar"] = "Sub Categories";
    ViewData["APICatalogDocumentationActive"] = "active";
    ViewData["APICatalogDocumentationShow"] = "show";
    ViewData["APISubCategoriesActive"] = "active";
}

@inject DropDown.ICommonDDL CommonDDL
@{

    var WebUrl = CommonDDL.GetWebURL();
}


<!-- end page title -->


<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h6 class="card-title">This section shows you all API endpoints to manage Sub Categories related data</h6>
                <div id="accordion" class="accordion" role="tablist">
                    <div class="card">
                        <div class="card-header" role="tab" id="headingOne">
                            <h6 class="mb-0">
                                <a data-toggle="collapse" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                                    <span class="text-black ">API/CategorySub/GetList</span> <span class="m-1 text-small text-left  text-small">(Get a list of Sub Categories records)</span>
                                </a>
                            </h6>
                        </div>
                        <div id="collapseOne" class="collapse " role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body">
                                <p class="card-text text-gray mb-2 ">This endpoint is used to list down the Sub Categories with filtering options and pagination.</p>
                                <p class="card-text text-gray">
                                    <ul class="card-text  text-gray mb-2">
                                        <li class="mb-2">Request structure: <b class="text-success"> {MainURL}/API/CategorySub/GetList?APIKey={Your_API_Key}&KeyW={Search_Keyword}&CategoryMainID={CategoryMain_ID}&Page={Page_Number}&PageSize={Records_Per_Page}</b>  </li>
                                        <li class="mb-2">Example request URL: <b class="text-primary">@WebUrl/API/CategorySub/GetList?APIKey=0000&KeyW=New&CategoryMainID=CTM_0000&Page=0&PageSize=99999</b></li>

                                        <li class="mb-2">
                                            Parameters
                                            <ul class="">

                                                <li class=""><b class="text-primary">APIKey </b><span class="text-danger">[Required]</span> - Your API Key</li>
                                                <li class=""><b class="text-primary">KeyW  </b><span class="text-info">[Optional - Default as '']</span> - Filter the list based on search keyword</li>
                                                <li class=""><b class="text-primary">CategoryMainID  </b><span class="text-info">[Optional - Default as '']</span> - Filter the list based on CategoryMain ID</li>
                                                <li class=""><b class="text-primary">Page  </b><span class="text-info">[Optional - Default as 0] </span> - Get records for specific page number with the help of PageSize parameter</li>
                                                <li class=""><b class="text-primary">PageSize   </b><span class="text-info">[Optional - Default as 99999]</span> - Set the number of records per page for this list</li>
                                            </ul>
                                        </li>

                                        <li class="mb-2">
                                            Results (JSon)
                                            <ul class="">

                                                <li class=""><b class="text-primary">Success </b>- True of false to indicate the success status of the request</li>
                                                <li class=""><b class="text-primary">ErrorMessage </b> - Displays any error message related to the request or parameters (If there is any error message, then success status will always show as false and Data eliment will not show) </li>
                                                <li class=""><b class="text-primary">Data </b> - Displays the list of records as JSon array if the request status is success. (This section will not show if the Success status is False)</li>
                                            </ul>
                                        </li>
                                    </ul>
                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-header" role="tab" id="headingTwo">
                            <h6 class="mb-0">
                                <a class="collapsed" data-toggle="collapse" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                    <span class="text-black ">API/CategorySub/GetCount </span> <span class="m-1 text-small text-left  text-small">(Get the number of Sub Categories records)</span>
                                </a>
                            </h6>
                        </div>
                        <div id="collapseTwo" class="collapse" role="tabpanel" aria-labelledby="headingTwo" data-parent="#accordion">
                            <div class="card-body">
                                <p class="card-text text-gray mb-2 ">This endpoint is used to get the number of Sub Categories  with filtering options </p>
                                <p class="card-text text-gray">
                                    <ul class="card-text  text-gray mb-2">
                                        <li class="mb-2">Request structure: <b class="text-success"> {MainURL}/API/CategorySub/GetCount?APIKey={Your_API_Key}&KeyW={Search_Keyword}&CategoryMainID={CategoryMain_ID}</b>  </li>
                                        <li class="mb-2">Example request URL: <b class="text-primary"> @WebUrl/API/CategorySub/GetCount?APIKey=0000&KeyW=New&CategoryMainID=CTM_0000</b></li>

                                        <li class="mb-2">
                                            Parameters
                                            <ul class="">

                                                <li class=""><b class="text-primary">APIKey </b><span class="text-danger">[Required]</span> - Your API Key</li>
                                                <li class=""><b class="text-primary">KeyW  </b><span class="text-info">[Optional - Default as '']</span> - Filter the list based on search keyword</li>
                                                <li class=""><b class="text-primary">CategoryMainID  </b><span class="text-info">[Optional - Default as '']</span> - Filter the list based on CategoryMain ID</li>
                                            </ul>
                                        </li>

                                        <li class="mb-2">
                                            Results (JSon)
                                            <ul class="">

                                                <li class=""><b class="text-primary">Success </b>- True of false to indicate the success status of the request</li>
                                                <li class=""><b class="text-primary">ErrorMessage </b> - Displays any error message related to the request or parameters (If there is any error message, then success status will always show as false and Data eliment will not show) </li>
                                                <li class=""><b class="text-primary">Data </b> -  Displays the numberic value as a result if the request status is success. (This section will not show if the Success status is False)</li>
                                            </ul>
                                        </li>
                                    </ul>
                                </p>
                            </div>
                        </div>
                    </div>


                    <div class="card">
                        <div class="card-header" role="tab" id="headingThree">
                            <h6 class="mb-0">
                                <a class="collapsed" data-toggle="collapse" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                    <span class="text-black ">API/CategorySub/Get</span> <span class="m-1 text-small text-left  text-small">(Get details of a single Sub Categories record)</span>
                                </a>
                            </h6>
                        </div>
                        <div id="collapseThree" class="collapse" role="tabpanel" aria-labelledby="headingThree" data-parent="#accordion">
                            <div class="card-body">
                                <p class="card-text text-gray mb-2 ">This endpoint is used to get details of a single Sub Categories records for any given CategorySub ID.</p>
                                <p class="card-text text-gray">
                                    <ul class="card-text  text-gray mb-2">
                                        <li class="mb-2">Request structure: <b class="text-success">{MainURL}/CategorySub/Get?APIKey={Your_API_Key}&ID={CategorySub_ID}</b>  </li>
                                        <li class="mb-2">Example request URL: <b class="text-primary">@WebUrl/API/CategorySub/Get?APIKey=0000&ID=BRD_000 </b></li>

                                        <li class="mb-2">
                                            Parameters
                                            <ul class="">

                                                <li class=""><b class="text-primary">APIKey </b><span class="text-danger">[Required]</span> - Your API Key</li>
                                                <li class=""><b class="text-primary">ID  </b><span class="text-danger">[Required]</span> - Used to find the specific Sub Categories using CategorySubID field</li>

                                            </ul>
                                        </li>

                                        <li class="mb-2">
                                            Results (JSon)
                                            <ul class="">

                                                <li class=""><b class="text-primary">Success </b>- True of false to indicate the success status of the request</li>
                                                <li class=""><b class="text-primary">ErrorMessage </b> - Displays any error message related to the request or parameters (If there is any error message, then success status will always show as false and Data eliment will not show) </li>
                                                <li class=""><b class="text-primary">Data </b> - Displays the details of the records as JSon array if the request status is success. (This section will not show if the Success status is False)</li>
                                            </ul>
                                        </li>
                                    </ul>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header" role="tab" id="headingfour">
                            <h6 class="mb-0">
                                <a class="collapsed" data-toggle="collapse" href="#collapsefour" aria-expanded="false" aria-controls="collapsefour">
                                    <span class="text-black ">API/CategorySub/AddEdit </span> <span class="m-1 text-small text-left  text-small">(Add or Edit a Sub Categories record)</span>
                                </a>
                            </h6>
                        </div>
                        <div id="collapsefour" class="collapse" role="tabpanel" aria-labelledby="headingfour" data-parent="#accordion">
                            <div class="card-body">
                                <p class="card-text text-gray mb-2 ">This endpoint is used add a new Sub Categories or update any Sub Categories record. If the CategorySubID parameter is null or empty, it adds the record, otherwise it will update the Sub Categories record.</p>
                                <p class="card-text text-gray">
                                    <ul class="card-text  text-gray mb-2">
                                        <li class="mb-2">Request structure: <b class="text-success">{MainURL}/CategorySub/AddEdit?APIKey={Your_API_Key}&CategorySubID={CategorySub_ID}&CategorySubName={CategorySub_Name}&CategoryMainID={CategoryMain_ID</b>  </li>
                                        <li class="mb-2">Example request URL: <b class="text-primary">  @WebUrl/API/CategorySub/AddEdit?APIKey=0000&CategorySubID={CategorySub_ID}&CategorySubName={CategorySub_Name}&CategoryMainID={CategoryMain_ID} </b></li>

                                        <li class="mb-2">
                                            Parameters
                                            <ul class="">

                                                <li class=""><b class="text-primary">APIKey </b><span class="text-danger">[Required]</span> - Your API Key</li>
                                                <li class=""><b class="text-primary">CategorySubID  </b><span class="text-danger">[Required]</span> - CategorySub ID (Set this as blank to add a new Organization)</li>
                                                <li class=""><b class="text-primary">CategorySubName </b><span class="text-danger">[Required]</span> - Name of the CategorySub</li>
                                                <li class=""><b class="text-primary">CategoryMainID </b><span class="text-danger">[Required]</span> - CategoryMainID which Sub Categories should create under </li>
                                            </ul>
                                        </li>

                                        <li class="mb-2">
                                            Results (JSon)
                                            <ul class="">

                                                <li class=""><b class="text-primary">Success </b>- True of false to indicate the success status of the request</li>
                                                <li class=""><b class="text-primary">ErrorMessage </b> - Displays any error message related to the request or parameters (If there is any error message, then success status will always show as false and Data eliment will not show) </li>
                                                <li class=""><b class="text-primary">Data </b> - Displays newly created CategorySub ID or updated CategorySub ID as a JSon result if the request status is success. (This section will not show if the Success status is False)</li>
                                            </ul>
                                        </li>
                                    </ul>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header" role="tab" id="headingfive">
                            <h6 class="mb-0">
                                <a class="collapsed" data-toggle="collapse" href="#collapsefive" aria-expanded="false" aria-controls="collapsefive">
                                    <span class="text-black ">API/CategorySub/Delete  </span> <span class="m-1 text-small text-left  text-small">(Delete Sub Categories record)</span>
                                </a>
                            </h6>
                        </div>
                        <div id="collapsefive" class="collapse" role="tabpanel" aria-labelledby="headingfive" data-parent="#accordion">
                            <div class="card-body">
                                <p class="card-text text-gray mb-2 ">This endpoint is used to delete a Sub Categories record. However if the Sub Categories is used for other records, it will display relevant message.</p>
                                <p class="card-text text-gray">
                                    <ul class="card-text  text-gray mb-2">
                                        <li class="mb-2">Request structure: <b class="text-success">{MainURL}/CategorySub/Delete?APIKey={Your_API_Key}&ID={CategorySub_ID}</b>  </li>
                                        <li class="mb-2">Example request URL: <b class="text-primary"> @WebUrl/API/CategorySub/Delete?APIKey=0000&ID=BRD_000 </b></li>

                                        <li class="mb-2">
                                            Parameters
                                            <ul class="">

                                                <li class=""><b class="text-primary">APIKey </b><span class="text-danger">[Required]</span> - Your API Key</li>
                                                <li class=""><b class="text-primary">ID  </b><span class="text-danger">[Required]</span> - Sub Categories ID of the Sub Categories to be deleted</li>

                                            </ul>
                                        </li>

                                        <li class="mb-2">
                                            Results (JSon)
                                            <ul class="">

                                                <li class=""><b class="text-primary">Success </b>- True of false to indicate the success status of the request</li>
                                                <li class=""><b class="text-primary">ErrorMessage </b> - Displays any error message related to the request or parameters (If there is any error message, then success status will always show as false and Data eliment will not show) </li>
                                                <li class=""><b class="text-primary">Data </b> - Displays deleted CategorySub ID as a JSon result if the request status is success. (This section will not show if the Success status is False)</li>
                                            </ul>
                                        </li>
                                    </ul>
                                </p>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>




</div>
